@model KasutajatugiPraktika.Models.Ticket

@{
ViewData["Title"] = "Lisa uus pöördumine";
}

<h1>Lisa uus pöördumine</h1>

<form asp-action="Create">
    
    
    <div class="form-group">
        <label asp-for="Description">Kirjeldus</label>
        <input asp-for="Description" class="form-control"/>
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>

    
    <div class="form-group">
        <label asp-for="Deadline">Tähtaeg</label>
        <input asp-for="Deadline" class="form-control" type="datetime-local"/>
        <span asp-validation-for="Deadline" class="text-danger"></span>
    </div>

    
    <button type="submit" class="btn btn-primary">Lisa</button>

</form>

    